<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jacklin.myshop.mapper.GoodsTypeMapper">
  <resultMap id="BaseResultMap" type="com.jacklin.myshop.entity.GoodsType">
    <id column="id" property="id" />
    <result column="typename" property="typename" />
  </resultMap>
  <delete id="deleteByPrimaryKey">
    delete from `tb_goodstype`
    where id = #{id}
  </delete>
  <insert id="insert" keyProperty="id" useGeneratedKeys="true">
    insert into `tb_goodstype` (typename)
    values (#{typename})
  </insert>
  <update id="updateByPrimaryKey">
    update `tb_goodstype`
    set typename = #{typename}
    where id = #{id}
  </update>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap">
    select id, typename
    from `tb_goodstype`
    where id = #{id}
  </select>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, typename
    from `tb_goodstype`
  </select>
    <select id="count" resultType="int">
        select count(*) from tb_goodstype
    </select>
</mapper>